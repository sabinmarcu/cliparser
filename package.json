{
	"name": "cliparser",
	"version": "0.0.1",
	"author": "Sabin Marcu <sabinmarcu@gmail.com>",
	"dependencies": {
		"typescript": "0.8.0",
		"coffee-script": "1.3.3",
		"jasmine-node": "1.0.26",
		"commander": "1.0.4",
		"colors": "0.6.0-1"
	},
	"engine": "0.6.12",
	"npm": "1.1.4",
	"scripts": {
		"start": "node lib/index.js",
		"compile": "./node_modules/typescript/bin/tsc ./src/parser.ts && mv ./src/parser.js ./lib/parser.js",
		"run": "npm run-script compile && npm start",
		"run-args": "npm run-script compile && node lib/index.js -o ./lib/parser.js --watch something wong -if you know",
		"test": "npm run-script compile && node ./node_modules/jasmine-node/bin/jasmine-node --coffee --color ./spec"
	}
}
